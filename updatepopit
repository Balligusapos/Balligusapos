local Tab = Window:NewTab("Fake Drop")
local Section = Tab:NewSection("Better Fake Drop")
Section:NewButton("Fake Drop", "ButtonInfo", function()
    local plr = game.Players.LocalPlayer
local chr = plr.Character

chr.RightHand:Destroy()
end)
Section:NewButton("Respawn", "ButtonInfo", function()
    local Player = game:GetService("Players").LocalPlayer
repeat wait() until Player.Character
local function Main()
	local Humanoid = Player.Character:WaitForChild("Humanoid")
	local ripfunction = false
	Humanoid.Died:Connect(function()
		local DeathLocation = Player.Character:WaitForChild("HumanoidRootPart").CFrame
		workspace.ChildAdded:Connect(function(Object)
			if not ripfunction and Object.Name == Player.Name then
				wait()
				Player.Character:WaitForChild("HumanoidRootPart").CFrame = DeathLocation
				ripfunction = true
				Main()
			end
		end)
	end)
end
Main()
wait()
game.Players.LocalPlayer.Character.Head:Destroy()
end)
