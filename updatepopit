local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("main script cool?")
Section:NewToggle("Anti Reap", "ToggleInfo", function(state)
    if state then
        _G.loop = true

while _G.loop do
local args = {
    [1] = "Watermelon"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
wait()
local args = {
    [1] = "Watermelon"
}

game:GetService("ReplicatedStorage").RemoteEvents.Equip:FireServer(unpack(args))
 wait(0.3)
end
    else
        _G.loop = false

while _G.loop do
local args = {
    [1] = "Watermelon"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
